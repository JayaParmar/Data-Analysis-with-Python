d=np.random.randint(0, 10, (4,4))
print(f"{d} has dim {d.ndim}, shape {d.shape}, size{d.size}, and dtype {d.dtype}:")

def get_rows(a):
    
    splitted_rows=np.vsplit(a,4)
    print(splitted_rows)
    
def get_columns(a):   
    
    splitted_columns=np.hsplit(a,4)
    print(splitted_columns)
    
get_rows(d)
get_columns(d)

